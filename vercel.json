{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/upload",
      "methods": [
        "POST",
        "OPTIONS"
      ],
      "dest": "app.py"
    },
    {
      "src": "/download/(.*)",
      "methods": [
        "GET"
      ],
      "dest": "app.py"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "functions": {
    "app.py": {
      "memory": 3008,
      "maxDuration": 300
    }
  }
}